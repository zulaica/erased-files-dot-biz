{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier", "jsdoc"],
  "rules": {
    // Rules options are managed by Prettier. Updates belong in Prettier's
    // configuration file: .prettierrc.json
  }
}
